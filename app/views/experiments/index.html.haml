.page-header
  %h1= raw t '.title'

= raw t '.info'

.row
  .col-sm-12
    %table.table.table-striped
      %thead
        %tr
          %th= t '.name'
          %th= t '.owner'
      %tbody
        - if @projects.blank?
          %tr
            %td{ colspan: 2 }= t '.nothing'
        - else
          - @experiments.each do |p|
            %tr
              %td= p.name
              %td= p.owner

